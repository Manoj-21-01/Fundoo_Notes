{
    "swagger": "2.0",
    "info": {
      "version": "V1",
      "title": "Fundoo_Notes App",
      "description": "CLI App API"
    },
    "host": "localhost:3000",
    "basePath": "/api/v1/",
    "schemes": [
      "http"
    ],
    "consumes": [
      "application/json"
    ],
    "produces": [
      "application/json"
    ],
    "paths": {
      "/user/registration": {
        "post": {
            "tags": ["Users"],
            "description": "register user...",
            "parameters": [
                {
                    "name": "register user",
                    "in": "body",
                    "description": "register",
                    "schema": {
                        "$ref": "#/definitions/User"
                    }
                }
            ],
            "produces": [
                "application/json"
            ],
            "responses": {
                "201": {
                    "description": "User created successfully"
                }
            }
        }
      },
      "/user/login": {
        "post": {
          "tags": ["Users"],
          "description": "login name...",
          "parameters": [
            {
              "name": "login user",
              "in": "body",
              "description": "login",
              "schema": {
                "$ref": "#/definitions/loging"
              }
            }
          ],
          "produces": [
            "application/json"
          ],
          "responses": {
            "201": {
              "description": "New user is created"
            },
            "500": {
              "description": "Internal server error"
            },
            "200": {
              "description": "Login successful"
            }
          }
        }
      },
      "/note": {
        "post": {
          "tags": ["Notes"],
          "description": "create a note...",
          "parameters": [
            {
              "name": "create note",
              "in": "body",
              "description": "register",
              "- in": "path",
              "schema": {
                "$ref": "#/definitions/Note"
              }
            }
          ],
          "produces": [
            "application/json"
          ],
          "consumes": [
            "application/json"
          ],
          "responses": {
            "201": {
              "description": "Note created successfully"
            }
          }
        }
      },
      "/note/{id}": {
        "get": {
          "tags": ["Notes"],
          "description": "get note...",
          "parameters": [
            {
              "name": "fetching note",
              "in": "path",
              "description": "register",
              "require": true
            }
          ],
          "produces": [
            "application/json"
          ],
          "consumes": [
            "application/json"
          ],
          "responses": {
            "201": {
              "description": "Note fetched successfully"
            }
          }
        }
      },
      "/note/update/noteId": {
        "put": {
          "tags": ["Notes"],
          "description": "updating the note...",
          "parameters": [
            {
              "name": "id",
              "- in": "path",
              "description": "id"
            },
            {
              "name": "update note",
              "in": "body",
              "schema": {
                "$ref": "#/definitions/Note"
              }
            }
          ],
          "produces": [
            "application/json"
          ],
          "consumes": [
            "application/json"
          ],
          "responses": {
            "201": {
              "description": "Note updated successfully"
            }
          }
        }
      },
      "/note/delete/{id}": {
        "delete": {
          "tags": ["Notes"],
          "description": "delete note...",
          "parameters": [
            {
              "name": "delete note",
              "in": "path",
              "description": "register",
              "schema": {
                "$ref": "#/definitions/Note"
              }
            }
          ],
          "produces": [
            "application/json"
          ],
          "consumes": [
            "application/json"
          ],
          "responses": {
            "201": {
              "description": "Note deleted successfully"
            }
          }
        }
      }
    },
    "definitions": {
      "User": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "emailid": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "loging": {
        "type": "object",
        "properties": {
          "emailid": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "Note": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string",
            "required": "true"
            }
          }
        }
    },
    "securityDefinitions": {
      "JWT": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    },
    "security": [
      {
        "JWT": []
      }
    ]
  }